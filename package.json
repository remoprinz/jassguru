{
  "name": "jasstafel-frontend",
  "private": true,
  "version": "2.8.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "clean": "rm -rf out && rm -f public/workbox-*.js* public/fallback-*.js* && find public -name 'fallback-*' -delete && rm -rf public/pwa && rm -f sw.js workbox-*.js fallback-*.js*",
    "build": "npm run clean && next build",
    "postbuild": "npm run sitemap:all",
    "sitemap:all": "npm run sitemap:guru && mv out/sitemap.xml out/sitemap-guru.xml && npm run sitemap:wiki",
    "sitemap:guru": "next-sitemap --config next-sitemap.config.cjs",
    "sitemap:wiki": "next-sitemap --config next-sitemap.wiki.config.cjs",
    "prebuild": "echo 'Prebuild step skipped'",
    "start": "next start",
    "lint": "eslint --ext .js,.ts . --fix",
    "analyze": "cross-env ANALYZE=true next build",
    "export": "next export",
    "validate": "node scripts/validate-deployment.js",
    "deploy": "npm run build && firebase deploy",
    "deploy:force": "npm run build && firebase deploy --only hosting",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "vitest run --coverage",
    "version:update": "node scripts/update-version.js",
    "check:deployment": "curl -s -I https://jassguru.ch/_next/static/css/ | grep -E 'content-type|status'",
    "health": "node scripts/health-check.js",
    "reprocess:images": "node scripts/reprocess-profile-images.mjs",
    "fix-permissions": "ts-node scripts/fix-permissions.ts",
    "fix-permissions:dry": "ts-node scripts/fix-permissions.ts --dry-run",
    "calculate-ratings:games": "node scripts/calculateRatingsPerGame.cjs",
    "calculate-ratings:games:group": "node scripts/calculateRatingsPerGame.cjs",
    "validate-jass-elo": "node scripts/validateJassElo.cjs",
    "reset-ratings": "node scripts/resetAndRecalculate.cjs",
    "show-leaderboard": "node scripts/showJassEloLeaderboard.cjs",
    "compress-screenshots": "node scripts/compress-screenshots.mjs",
    "migrate-player-data": "ts-node functions/scripts/migratePlayerDataPhase1.ts",
    "migrate-player-data-dry": "ts-node functions/scripts/migratePlayerDataPhase1.ts --dry-run",
    "migrate-player-data-group": "ts-node functions/scripts/migratePlayerDataPhase1.ts --group-id=Tz0wgIHMTlhvTtFastiJ",
    "cleanup-old-data": "ts-node functions/scripts/cleanupOldPlayerData.ts",
    "cleanup-old-data-dry": "ts-node functions/scripts/cleanupOldPlayerData.ts --dry-run",
    "cleanup-old-data-group": "ts-node functions/scripts/cleanupOldPlayerData.ts --group-id=Tz0wgIHMTlhvTtFastiJ",
    "safe-cleanup-old-data": "ts-node functions/scripts/safeCleanupOldPlayerData.ts",
    "safe-cleanup-old-data-dry": "ts-node functions/scripts/safeCleanupOldPlayerData.ts --dry-run",
    "safe-cleanup-old-data-group": "ts-node functions/scripts/safeCleanupOldPlayerData.ts --group-id=Tz0wgIHMTlhvTtFastiJ",
    "backfill-player-data": "ts-node functions/scripts/backfillPlayerDataFromSummaries.ts",
    "backfill-player-data-dry": "ts-node functions/scripts/backfillPlayerDataFromSummaries.ts --dry-run",
    "backfill-player-data-group": "ts-node functions/scripts/backfillPlayerDataFromSummaries.ts --group-id=Tz0wgIHMTlhvTtFastiJ",
    "backfill-player-data-player": "ts-node functions/scripts/backfillPlayerDataFromSummaries.ts --player-id=",
    "restore-globalStats-current": "cd functions && ts-node --project tsconfig.json scripts/restore-globalStats-current.ts",
    "restore-globalStats-current-dry": "cd functions && ts-node --project tsconfig.json scripts/restore-globalStats-current.ts --dry-run",
    "check-globalStats": "cd functions && ts-node --project tsconfig.json scripts/check-globalStats.ts"
  },
  "engines": {
    "node": ">=18.0.0 <23.0.0",
    "npm": ">=8.0.0"
  },
  "main": "lib/index.js",
